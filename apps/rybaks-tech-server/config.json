{
  "s3": {
    "region": "#{S3_REGION}",
    "endpoint": "#{S3_ENDPOINT}",
    "credentials": {
      "accessKeyId": "#{S3_ACCESS_KEY_ID}",
      "secretAccessKey": "#{S3_SECRET_ACCESS_KEY}"
    },
    "bucketName": "#{S3_BUCKET_NAME}"
  },
  "Sequelize": [
    {
      "name": "rybaksTech",
      "options": {
        "dialect": "postgres",
        "database": "#{POSTGRES_DB_NAME}",
        "username": "#{POSTGRES_USER}",
        "password": "#{POSTGRES_PASSWORD}",
        "host": "#{POSTGRES_HOST}",
        "port": "#{POSTGRES_PORT}",
        "ssl": false,
        "pool": {
          "max": 5,
          "min": 0,
          "idle": 10000
        }
      }
    }
  ]
}
