{
  "name": "rybaks-tech-server",
  "version": "0.0.0",
  "description": "My app name (server application)",
  "main": "dist",
  "scripts": {
    "start": "cross-env LOG_ENABLED=1 NODE_ENV=production node ./dist/index.js",
    "build": "cross-env NODE_ENV=production npx tsc -p ./tsconfig.build.json --outDir ./dist",
    "start:dev": "cross-env NODE_ENV=dev npx nodemon --exec ts-node index.ts",
    "test": "npx nyc -- npx mocha",
    "migrations": "node -r @biorate/migrations",
    "lint:fix": "npx eslint --fix ./src",
    "prettier:fix": "npx prettier --write ./src",
    "allure:report": "npx allure serve allure-results --clean -o allure-report",
    "hooks:pre-commit": "pnpm run prettier:fix && pnpm run lint:fix"
  },
  "keywords": [],
  "license": "UNLICENSED",
  "dependencies": {
    "@biorate/axios-prometheus": "1.96.2",
    "@biorate/config-loader": "1.96.0",
    "@biorate/config-loader-env": "1.96.0",
    "@biorate/config-loader-fs": "1.96.0",
    "@biorate/config-loader-vault": "1.96.0",
    "@biorate/nestjs-tools": "1.97.0",
    "@biorate/pg": "^1.96.0",
    "@biorate/prometheus": "1.96.0",
    "@biorate/sequelize": "^1.96.0",
    "@biorate/vault": "1.96.0",
    "@nestjs/common": "10.4.15",
    "@nestjs/core": "10.4.15",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "8.1.0",
    "argon2": "^0.41.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.5",
    "express": "4.18.2",
    "helmet": "4.6.0",
    "http-proxy-middleware": "2.0.6",
    "multer": "1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "6.37.3",
    "serve-favicon": "2.5.0",
    "sharp": "^0.33.5",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "4.6.2"
  },
  "devDependencies": {
    "@biorate/migrations": "1.96.2",
    "@biorate/mocha": "1.64.0",
    "@biorate/mocha-spec": "1.64.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/mocha": "9.1.1",
    "@types/multer": "^1.4.12",
    "@types/passport-jwt": "^4.0.1",
    "mocha": "10.2.0",
    "mocha-chai-jest-snapshot": "1.1.4",
    "mocha-multi-reporters": "1.5.1",
    "nodemon": "3.1.3",
    "testcontainers": "10.9.0"
  }
}
